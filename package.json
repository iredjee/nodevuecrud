{
  "name": "interview",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=dev node api",
    "start:dev:watch": "cross-env NODE_ENV=dev nodemon api",
    "start:prod": "cross-env NODE_ENV=prod node api",
    "build": "npm run build:dev",
    "build:dev": "npm run build:clean && cross-env NODE_ENV=development webpack",
    "build:dev:watch": "npm run build:clean && cross-env NODE_ENV=development webpack --watch",
    "build:prod": "npm run build:clean && cross-env NODE_ENV=production webpack",
    "build:clean": "rm -rf client/dist",
    "test": "npm run test:api",
    "test:api": "npm run test:api:integration",
    "test:api:integration": "cross-env NODE_ENV=test nyc mocha --timeout=120000 --exit api/tests/integration/*.test.js",
    "clean": "npm run api:clean && npm run coverage:clean && npm run build:clean",
    "api:server": "node api/swagger",
    "api:docs": "spectacle -d ./api/swagger.yml -t ./api/docs",
    "api:clean": "rm -rf api/docs",
    "coverage:clean": "rm -rf .nyc_output"
  },
  "engines": {
    "node": ">=8.9"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "joi": "^13.3.0",
    "mongoose": "^5.1.1",
    "vue": "^2.5.16",
    "vue-notification": "^1.3.10",
    "vue-router": "^3.0.1",
    "winston": "^3.0.0-rc5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0",
    "sass-loader": "^7.0.1",
    "spectacle-docs": "^1.0.3",
    "style-loader": "^0.21.0",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "vue-loader": "^15.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  }
}
