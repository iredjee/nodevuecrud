{
  "name": "interview",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=dev node api",
    "start:dev:watch": "cross-env NODE_ENV=dev nodemon api",
    "start:prod": "cross-env NODE_ENV=prod node api",
    "test": "npm run test:api",
    "test:api": "npm run test:api:integration",
    "test:api:integration": "cross-env NODE_ENV=test nyc mocha --timeout=120000 --exit api/tests/integration/*.test.js",
    "clean": "npm run api:clean && npm run coverage:clean",
    "api:docs": "spectacle -d ./api/swagger.yml -t ./api/docs",
    "api:clean": "rm -rf api/docs",
    "coverage:clean": "rm -rf .nyc_output"
  },
  "engines": {
    "node": ">=8.9"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "joi": "^13.3.0",
    "mongoose": "^5.1.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^5.1.5",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0",
    "spectacle-docs": "^1.0.3"
  }
}
